{
  "name": "cinderblock",
  "version": "0.0.1",
  "description": "A new programming language",
  "scripts": {
    "build": "tsc",
    "package": "pkg .",
    "test:chained-operators": "npm run build && cd ../cinderblock-tests/chained-operators && cinderblock compile",
    "test:if-expressions": "npm run build && cd ../cinderblock-tests/if-expressions && cinderblock compile"
  },
  "private": true,
  "bin": {
    "cinderblock": "./bin/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cinderblock-lang/cinderblock-compiler.git"
  },
  "author": "Paul Taylor",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cinderblock-lang/cinderblock-compiler/issues"
  },
  "homepage": "https://github.com/cinderblock-lang/cinderblock-compiler#readme",
  "devDependencies": {
    "@types/node": "^20.8.10",
    "pkg": "^5.8.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "yargs": "^17.7.2",
    "zod": "^3.22.4"
  },
  "pkg": {
    "targets": [
      "node18-linux-x64"
    ],
    "outputPath": "pkg"
  }
}
